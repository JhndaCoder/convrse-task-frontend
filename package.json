{
  "name": "convrse-front",
  "private": true,
  "version": "0.0.0",
  "description": "Frontend for the Convrse real estate platform, featuring property listing and booking with Stripe integration.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start:prod": "vite preview --port 4173",
